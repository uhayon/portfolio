{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "rules": {
      "linebreak-style": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js", ".jsx"]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "es5",
          "singleQuote": true,
          "printWidth": 100
        }
      ]
    },
    "plugins": ["prettier"]
  }